/**index.wxss**/
@import "/icon.wxss";
@import "/base-rpx.wxss";
page{
  width: 710rpx;
  padding: 0 20rpx;
  padding-top: 21rpx;
  background-color: #fff;
}
.coupon_codearea .coupon_codetext {
  display: inline-block;
  width: 100%;
  text-align: center;
  font-size: 26rpx;
  font-weight: bold;
}
.coupon_codearea .coupon_qrcode{
  width: 400rpx;
  height: 400rpx;
  margin-left:160rpx;
  margin-top: 20rpx;
}
.coupon_codearea .coupon_barcode{
  width: 400rpx;
  margin-left: 105rpx;
  margin-bottom: 10rpx;
  margin-top: 70rpx;
}
.coupon_warning image{
  width: 300rpx;
  height: 300rpx;
  margin-left: 225rpx;
  margin-top: 70rpx;
}
.coupon_verify image{
  width: 200rpx;
  height: 200rpx;
  margin-left: 275rpx;
  margin-top: 70rpx;
}
.coupon_tips_normal,.coupon_tips_verifyed,.coupon_warning text{
  display:inline-block;
  width:100%;
  text-align:center;
  font-size:26rpx;
  font-weight:bold;
  padding:50rpx 0;
  color:#999;
}
.coupon_verify .coupon_tips_verifyed {
  color: #f0ba80;
  font-size:32rpx;
}
.table {
  color: #666;
  border-top: 1px solid #f4f4f4;
  padding-top: 20rpx;
}
.table .table-title {
  font-size:14px;
  font-weight:bold;
}
.table .list{
  overflow: hidden;
  margin: 20rpx 0;
}
.table .list text{
  font-size: 24rpx;
  padding-left: 10rpx;
  float: left;
  position: relative;
}
.table .list>text:first-child{
  font-weight: bold;
  padding-left: 0;
}
.table .list view{
  float: left;
  width: 500rpx;
}
.table .list view text{
  display: inline-block;
  width: 100%;
}
.table .list rich-text{
  font-size: 24rpx;
  width:550rpx;
  display:block;
  float:left;
  line-height: 1.75;
  padding:0 10rpx;
}
.coupon{
  overflow: hidden;
  margin-bottom: 20rpx;
  background: #F5F5F5;
  border-radius: 4px;
}
.coupon .money{
  width:210rpx;
  height:170rpx;
  border-radius:4px;
  overflow:hidden;
  display:inline-block;
  float:left;
  text-align:center;
  color: #F86060;
}
.coupon .money text {
  font-size:32px;
  line-height:50px;
  height:50px;
  display:inline-block;
  font-weight:bolder;
  margin-top:36rpx;
}
.coupon .money .fix{
  font-size: 16px;
  display: inline-block;
}
.coupon .info {
  width: 500rpx;
  height:170rpx;
  display:inline-block;
  float:left;
}
.coupon .info .coupon_title{
  font-size: 17px;
  color: #000000;
  font-weight: bold;
  height: 14px;
  line-height: 14px;
  display: block;
  margin-top: 18px;
}
.coupon .info .coupon_jingle {
  display: block;
  height: 18px;
  margin-top: 8px;
  overflow: hidden;
}
.coupon .info .coupon_jingle text{
  font-size: 11px;
  padding: 0 4px;
  width:auto;
  height: 18px;
  line-height: 20px;
  color: #F86060;
  background: #FCEBEB;
  border-radius: 4px;
  overflow: hidden;
  display: inline-block;
}
.coupon .info .coupon_time{
  font-size: 11px;
  color: #BABABA;
  height: 18px;
  line-height: 20px;
  display: block;
  margin-top: 5px;
}
.click{
  -webkit-filter: grayscale(100%);
  -moz-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: grayscale(100%);
  filter: gray;
}
.money{
  color: #ED6D68;
}
.end_time{
  width: 485rpx;
  position: absolute;
  left: 35rpx;
  bottom: 30rpx;
  font-size: 20rpx;
  color: #9C9B98;
}
.btn-receive {
  height:80rpx;
  width:710rpx;
  border:none;
  line-height:86rpx;
  font-size:28rpx;
  border-radius:8rpx;
  position:absolute;
  bottom:40rpx;
  text-align:center;
}
.btn-receive.receive {
  background: url("data:image/png;base64,") no-repeat center;
  color:#fff;
}

.btn-receive.received {
  background: #e9e9e9;
  color: #ccc;
}

/*店铺*/
.font_grey{
    color:#707070;
}
.border_bottom {
  border-bottom:1px solid #F4F4F4;
}
.padding30{
  padding:30rpx 0;
}
.content{
  margin-bottom: 20rpx;
  width: 100%;
  border-radius: 10rpx;
  box-shadow: 2rpx 2rpx rgba(160,160,158,.18),-2rpx -2rpx rgba(160,160,158,.18);
  background-color: #fff;
}
.content{
  padding: 10rpx 30rpx;
  height: 396rpx;
}
.content .shop_img{
  margin-right: 30rpx;
  width: 150rpx;
  height: 110rpx;
  border-radius: 4rpx;
}
.shop_name{
  margin-top:24rpx;
  margin-bottom: 20rpx;
}
.distance{
  font-size: 24rpx;
}
.icon_distance{
  display: inline-block;
  margin-right: 10rpx;
  width: 15rpx;
  height:19rpx ;
}
.address_wrap{
  position: relative;
}
.icon,.icon_wrap{
  display: inline-block;
  border-left: 1px solid #F4F4F4;
  padding:0 36rpx;
  background-repeat: no-repeat; 
  background-position: center;
}
.address{
  max-width: calc(100% - 100rpx);
  line-height: 40rpx;
}
.icon_wrap{
  position: absolute;
  top:0;
  right: 0;
  margin:30rpx 0;
  padding:0 20rpx;
  height: calc(100% - 60rpx);
}
.icon_navigation{
  width: 29rpx;
  height: 30rpx; 
  background-size: 29rpx 30rpx;
  background-image: url(https://youbao-1251769479.cos.ap-chengdu.myqcloud.com/oil_clean/icon_navigation.png);
}
.icon_phone{
  width: 26rpx;
  height: 26rpx;
  background-size: 26rpx 26rpx;
  background-image: url(https://youbao-1251769479.cos.ap-chengdu.myqcloud.com/oil_clean/icon_phone.png)
}
/**index.wxss**/
page,#coupon{
  width: 710rpx;
  padding: 0 20rpx;
}
.shop{
  overflow: hidden;
  position: relative;
}
.deduction{
  border-bottom: 1px solid #F4F4F4;
  padding: 1em 0;
  overflow: hidden;
}
.shop>image{
  float: left;
  width: 180rpx;
  height: 180rpx;
}
.shop_content{
  width: 100%;
  position: relative;
  padding: 35rpx 0;
  border-bottom: 1px solid #F4F4F4;
}
.shop_content .title{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
.service{
  font-size: 24rpx;
  color: #959595;
}
.money{
  font-size: 36rpx;
  color: #E0CA9E;
  right: 0;
  bottom:30rpx;
  font-weight: bold;
}
.count{
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.count image{
  width: 1.5em;
  height: 1.5em;
  float: left;
}
.count text{
  display: inline-block;
  font-size: 24rpx;
  color: #939394;
  text-align: center;
}
.deduction view{
  float: right;
}
.deduction image{
  width: 0.5em;
  height: 0.5em;
  margin-left: 0.5em;
}
.deduction>text{
  font-size: 0.9em;
}
.more text{
  font-size: 0.7em;
  color: #B9B9B9;
  position: relative;
  top: -0.1em;
}
#money{
  font-size: 26rpx;
  padding: 1.2em 0;
  border-bottom: 1px solid #F4F4F4;
  margin-bottom: 2em;
}
#money view{
  padding: 18rpx 0;
  overflow: hidden;
}
#money text{
  float: right;
  font-weight: bold;
  font-size: 30rpx;
}
#money text.pl{
  font-weight: normal;
  color: #B2B2B2;
  font-size: 24rpx;
}
.total{
  color: #DA5E5E;
}
button{
  width: 690rpx;
  color: white;
  margin-left: 10rpx;
  background-color: #4B70F8;
  font-size: 26rpx;
  text-align: center;
  margin-top: 29rpx;
  border-radius: 4rpx;
  height: 80rpx;
  line-height: 80rpx;
}
button:active{
  background-color: #7993F7;
  color: white;
}
button.wei{
  background-color: #51AA38;
  margin-top: 49rpx;
}
button.wei:active{
  background-color: #5AC140;
  color: white;
}
#fixed{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 100;
}
#coupon{
  position: fixed;
  height: 80%;
  left: 0;
  bottom: 0;
  background-color: white;
  z-index: 110;
}
#title{
  text-align: center;
  overflow: hidden;
  font-size: 0.8em;
  padding: 0.7em 0;
  border-bottom: 1px solid #F4F4F4;
}
#title image{
  width: 1em;
  float: right;
  margin-right: 3%;
}

.coupon{
  overflow: hidden;
  padding: 0.5em 0;
  border-bottom: 1px solid #F4F4F4;
  position: relative;
}
.image_{
  width: 115rpx;
  margin-left: 21rpx;
  float: left;
  position: relative;
}
.image_ image{
  width: 115rpx;
  height: 157rpx;
}
.image_ view{
  width: 100%;
  position: absolute;
  top: 35%;
  left: 0;
  text-align: center;
  color: white;
  font-size: 36rpx;
}
.image_ text{
  font-size: 22rpx;
}
.content{
  float: left;
  width: 70%;
  margin-left: 3%;
  margin-top: 20rpx;
  font-weight: bold;
  overflow: hidden;
}
.content text{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  float: left;
  font-size: 28rpx;
}
.content text.de_,.content text.de{
  font-size: 22rpx;
  font-weight: normal;
  display: inline-block;
  margin-top: 10rpx;
}
.de{
  color: #D9D9D9;
}
#coupon_out{
  height: 90%;
}
.checked{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 50%;
  margin-top: -15rpx;
  right: 5%;
}

#fixed_{
  position: fixed;
  width: 690rpx;
  left: 30rpx;
  top: 8%;
  background-color: white;
  border-radius: 10rpx;
}
#bg_{
  width: 100%;
}
#view_in{
  position: absolute;
  top: 305rpx;
  width: 608rpx;
  left: 41rpx;
}
.list_in{
  width: 100%;
  position: relative;
  margin-bottom: 10rpx;
  overflow: hidden;
}
.list_in image{
  width: 100%;
  float: left;
}
.list_in_{
  position: absolute;
}
.list_in_1{
  font-size: 36rpx;
  color: #FF4D4D;
  top: 32rpx;
  left: 36rpx;
}
.list_in_2{
  font-size: 72rpx;
  color: #FF4D4D;
  top: 49rpx;
  left: 75rpx;
  font-weight: bold;
}
.list_in_3{
  font-size: 28rpx;
  color: #1C1C1C;
  font-weight: bold;
  top: 33rpx;
  left: 185rpx;
}
.list_in_4{
  font-size: 22rpx;
  color: #AFAFAF;
  top: 81rpx;
  left: 185rpx;
}
.list_in_5{
  width: 160rpx;
  height: 57rpx;
  line-height: 57rpx;
  background-color: #FF4D4D;
  font-size: 26rpx;
  color: white;
  border-radius: 28rpx;
  top: 42rpx;
  right: 32rpx;
  text-align: center;
}
.list_in_6{
  font-size: 22rpx;
  color: #AFAFAF;
  left: 46rpx;
  bottom: 20rpx;
}

#list_closed{
  width: 56rpx;
  position: absolute;
  left: 317rpx;
  bottom:-70rpx;
}
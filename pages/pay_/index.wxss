@import "/icon.wxss";
page{
  width: 750rpx;
  padding: 0;
  background-color: #F4F4F4;
}
#coupon{
  width: 710rpx;
  padding: 0 20rpx;
}
.shop{
  overflow: hidden;
  position: relative;
  padding: 0 20rpx;
  background-color: white;
  margin-bottom:16rpx;
  border-bottom:1px solid #eee;
}
.deduction{
  border-bottom: 1px solid #F4F4F4;
  padding: 30rpx 0;
  overflow: hidden;
  background-color: white;
  display: block;
}
.shop>image{
  float: left;
  width: 180rpx;
  height: 180rpx;
}
.shop_content{
  width: 100%;
  position: relative;
  padding: 35rpx 0;
  border-bottom: 1px solid #F4F4F4;
}
.shop_content .title{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
  font-size: 32rpx;
}
.service{
  font-size: 24rpx;
  color: #959595;
}
.money{
  display: inline-block;
  font-size: 32rpx;
  color: #E0CA9E;
  margin-top: 25rpx;
  font-weight: bold;
}
.number{
  position: absolute;
  right: 0;
  bottom: 40rpx;
  color: #939394;
  font-size: 24rpx;
}
.count{
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.count image{
  width: 1.5em;
  height: 1.5em;
  float: left;
}
.count text{
  display: inline-block;
  font-size: 24rpx;
  color: #939394;
  text-align: center;
}
.deduction view{
  float: right;
}
.deduction image{
  width: 11rpx;
  height: 18rpx;
  margin-left: 0.5em;
}
.deduction>text{
  font-size: 30rpx;
}
.more text{
  font-size: 28rpx;
  color: #979696;
  position: relative;
  top: -0.1em;
  display: inline-block;
}
.button_all{
  background-color: white;
  padding-top: 44rpx;
  padding-bottom: 83rpx;
}
#money{
  font-size: 26rpx;
  padding: 0 20rpx;
  border-bottom: 1px solid #F4F4F4;
  background-color: white;
}
#money view{
  padding: 18rpx 0;
  overflow: hidden;
}
#money text{
  float: right;
  font-weight: bold;
  font-size: 30rpx;
}
#money text.pl{
  font-weight: normal;
  color: #B2B2B2;
  font-size: 24rpx;
}
.total{
  color: #DA5E5E;
}
button{
  width: 690rpx;
  color: white;
  margin-left: 30rpx;
  background-color: #4B70F8;
  font-size: 26rpx;
  text-align: center;
  margin-top: 29rpx;
  border-radius: 4rpx;
  height: 80rpx;
  line-height: 80rpx;
}
button:active{
  background-color: #7993F7;
  color: white;
}
button.wei{
  background-color: #51AA38;
  margin-top: 0;
}
button.wei:active{
  background-color: #5AC140;
  color: white;
}
#fixed{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 100;
}
#coupon{
  position: fixed;
  height: 80%;
  left: 0;
  bottom: 0;
  background-color: white;
  z-index: 110;
}
#title{
  text-align: center;
  overflow: hidden;
  font-size: 30rpx;
  font-weight: bold;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #F4F4F4;
}
#title image{
  width: 22rpx;
  height: 22rpx;
  float: right;
  margin-right: 21rpx;
  margin-top: 35rpx;
}

.coupon{

#firstLoginModal {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.7);
  z-index: 9999;
  text-align: center;
}
#firstLoginModal .modal-container {
  width: 450rpx;
  margin: 200rpx 150rpx 0 150rpx;
  background:url("https://youbao-1251769479.cos.ap-chengdu.myqcloud.com/first_login_modal/modal_container.png") top center no-repeat;
  background-size: 100%;
  text-align: center;
  padding-top: 550rpx;
  position: relative;
}
#firstLoginModal .modal-footer .modal-receive{
  background: #FECB47 !important;
  font-size: 28rpx;
  color: #A34B18;
  width: 300rpx;
  height: 68rpx;
  border-radius: 68rpx;
  letter-spacing:4rpx;
  text-shadow:1px 1px 1px #fff;
  margin:0;
  position:relative;
  display:inline-flex;
  align-items:center;
  justify-content:center;
  box-sizing:border-box;
  padding:20rpx 30rpx 16rpx;
}
#firstLoginModal .modal-footer .modal-receive:before {
  position: absolute;
  content: "";
  top: 5rpx;
  left: 5rpx;
  width: 190rpx;
  height: 58rpx;
  border-radius: 68rpx;
  background:linear-gradient(to top, #FECB47, #FED66B);
  /* box-shadow: 0 30rpx 20rpx rgba(0,0,0,.2); */
  z-index: -1;
}
#firstLoginModal .modal-footer .modal-receive:after {
  border: none !important;
  box-shadow: 0 12rpx 2rpx #E9922D;
  z-index: -1;
}
#firstLoginModal .modal-action {
  font-size: 48rpx;
  color: #999;
  position: absolute;
  top:-60rpx;
  right:0;
}
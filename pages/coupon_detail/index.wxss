/**index.wxss**/
page{
  width: 710rpx;
  padding: 0 20rpx;
  padding-top: 21rpx;
}
.table {
  color: #666;
  border-top: 1px solid #f4f4f4;
  padding-top: 20rpx;
  padding-bottom:120rpx;
}
.table .table-title {
  font-size:14px;
  font-weight:bold;
}
.table .list{
  overflow: hidden;
  margin: 20rpx 0;
}
.table .list text{
  font-size: 24rpx;
  padding-left: 10rpx;
  float: left;
  position: relative;
}
.table .list>text:first-child{
  font-weight: bold;
  padding-left: 0;
}
.table .list view{
  float: left;
  width: 500rpx;
}
.table .list view text{
  display: inline-block;
  width: 100%;
}
.table .list rich-text{
  font-size: 24rpx;
  width:550rpx;
  display:block;
  float:left;
  line-height: 1.75;
  padding:0 10rpx;
}




.coupon{
  overflow: hidden;
  margin-bottom: 20rpx;
  background: #F5F5F5;
  border-radius: 4px;
}
.coupon .money{
  width:210rpx;
  height:170rpx;
  border-radius:4px;
  overflow:hidden;
  display:inline-block;
  float:left;
  text-align:center;
  color: #F86060;
}
.coupon .money text {
  font-size:32px;
  line-height:50px;
  height:50px;
  display:inline-block;
  font-weight:bolder;
  margin-top:36rpx;
}
.coupon .money .fix{
  font-size: 16px;
  display: inline-block;
}
.coupon .info {
  width: 500rpx;
  height:170rpx;
  display:inline-block;
  float:left;
}
.coupon .info .coupon_title{
  font-size: 17px;
  color: #000000;
  font-weight: bold;
  height: 14px;
  line-height: 14px;
  display: block;
  margin-top: 18px;
}
.coupon .info .coupon_jingle {
  display: block;
  height: 18px;
  margin-top: 8px;
  overflow: hidden;
}
.coupon .info .coupon_jingle text{
  font-size: 11px;
  padding: 0 4px;
  width:auto;
  height: 18px;
  line-height: 20px;
  color: #F86060;
  background: #FCEBEB;
  border-radius: 4px;
  overflow: hidden;
  display: inline-block;
}
.coupon .info .coupon_time{
  font-size: 11px;
  color: #BABABA;
  height: 18px;
  line-height: 20px;
  display: block;
  margin-top: 5px;
}

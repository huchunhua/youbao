.click{
  -webkit-filter: grayscale(100%);
  -moz-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: grayscale(100%);
  filter: gray;
}
.money{
  /*position: absolute;*/
  /*top: 33rpx;*/
  /*right: 32rpx;*/
  color: #ED6D68;
}
.end_time{
  width: 485rpx;
  position: absolute;
  left: 35rpx;
  bottom: 30rpx;
  font-size: 20rpx;
  color: #9C9B98;
}



.shop{
  overflow: hidden;
  border-bottom: 1px solid #F4F4F4;
  padding: 20rpx 0;
  margin-top:20rpx;

}
.shop:first-child{
  padding-top: 0;
}
.shop:last-child{
  border: none;
}
.shop>image{
  float: left;
  width: 170rpx;
  height: 170rpx;
}
.shop_content{
  float: left;
  width:510rpx;
  margin-left: 20rpx;
  position: relative;
  overflow: hidden;
}
.shop_content>text,.shop_content>view{
  float: left;
}
.shop_content image{
  width: 29rpx;
  height: 29rpx;
  position: relative;
  top: 0.07em;
  margin-right: 0.3em;
}
.shop_content text.distance{
  width: auto;
  text-align: center;
  font-size: 22rpx;
  height: 36rpx;
  line-height: 36rpx;
  background-color: #EFEFEF;
  color: #707070;
  padding: 0 0.4em;
  margin-top: 10rpx;
  border-radius:4px;
}
.shop_content view{
  width: 100%;
  font-size: 26rpx;
  color: #888888;
  margin-top: 10rpx;
}

.shop_content view:first-child{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
  font-size: 30rpx;
  margin-top: 0;
  color: black;
}
.shop_content view:first-child image{
  margin-left: 10rpx;
  width: 30rpx;
  height: 30rpx;
  margin-top: 4rpx;
}
.shop_content view:last-child{
  font-size: 24rpx;
  color: #B8B8B8;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.btn-panel {
  position:fixed;
  padding:20rpx;
  left:0;
  bottom:0;
  background:#fff;
}
.btn-panel .btn-receive {
  height:80rpx;
  width:710rpx;
  border:none;
  line-height:86rpx;
  font-size:28rpx;
  border-radius:8rpx;
  text-align:center;
}

.btn-panel .btn-receive.receive {
  background: url("data:image/png;base64,") no-repeat center;
  color:#fff;
}

.btn-panel .btn-receive.received {
  background: #e9e9e9;
  color: #ccc;
}

.seller_oil text{
  color: #e2c895;
  border: 1px solid #e2c895;
  float: left;
  width: 50rpx;
  text-align: center;
  font-size: 22rpx;
  margin-right: 8rpx;
  border-radius: 2px;
}